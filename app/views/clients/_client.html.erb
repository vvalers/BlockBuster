<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movies.last %></td>
    <td><%= link_to "Ver", client_path(client), class: "btn btn-info" %></td>
    <td><%= link_to "Editar", edit_client_path(client), class: "btn btn-warning" %></td>
    <td><%= button_to "Eliminar", client, method: :delete, class: "btn btn-danger" %></td>
  </tr>
</div>
